CREATE TABLE ifwefoosball.import (
	import_ID int(10) NOT NULL auto_increment,
	player1 varchar(100),
	player1Score int(10),
	player2 varchar(100),
	player2Score int(10),
	ImportDate datetime,
	PRIMARY KEY (import_ID)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
CREATE TABLE ifwefoosball.ranking (
	ranking_ID int(10) NOT NULL auto_increment,
	player varchar(100),
	playerScore int(10),
	playerTotalGames int(10),
	playerHighestScore int(10),
	PRIMARY KEY (ranking_ID)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
